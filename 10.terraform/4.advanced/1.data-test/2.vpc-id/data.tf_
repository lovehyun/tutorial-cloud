provider "aws" {
  region = "us-east-1"
}

# 특정 태그나 이름을 기준으로 VPC ID 가져오기
data "aws_vpcs" "selected" {
  filter {
    name   = "is-default"
    values = ["true"]
  }
}

# 선택된 VPC의 서브넷 ID 가져오기
data "aws_subnets" "selected" {
  filter {
    name   = "vpc-id"
    values = data.aws_vpcs.selected.ids
  }
}

output "vpc_ids" {
  value = data.aws_vpcs.selected.ids
}

output "subnet_ids" {
  value = data.aws_subnets.selected.ids
}
